    "@heroicons/react": "^1.0.6",
    "emoji-mart": "^3.0.1",
    "firebase": "^9.9.2",
    "moment": "^2.29.4",
    "next": "^12.1.6",
    "next-auth": "^4.10.3",
    "react": "18.1.0",
